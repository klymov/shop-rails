<%= form_tag("/items", method: "get") do %>
  <%= text_field_tag (:search), params[:search] %>
  <% if (params[:category_id]) %>
    <%= hidden_field_tag (:category_id), params[:category_id] %>
  <% end %>
  <%= submit_tag 'Search' %>
<% end %>
